{
  "name": "ersa-reporting-frontend",
  "version": "2.0.0",
  "description": "eRSA Reporting Frontend",
  "repository": {
    "type": "git",
    "url": "https://github.com/eresearchsa/reporting-frontend"
  },
  "devDependencies": {
    "browser-sync": "^2.13.0",
    "eslint": "^2.0.0",
    "jasmine-core": "^2.4.1",
    "karma": "~1.5",
    "karma-jasmine": "^1",
    "karma-phantomjs-launcher": "^1",
    "karma-chrome-launcher": "^1",
    "karma-requirejs": "^1",
    "requirejs": "^2.2.0"
  },
  "dependencies": {
    "bower": "^1.7.9",
    "animate.css": "^3.5.2",
    "qs": "^6.1.0",
    "dc": "~3.0.6"
  },
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "lint": "eslint js tests",
    "test": "karma start karma.conf.js --single-run",
    "testwatch": "karma start karma.conf.js --no-single-run --auto-watch",
    "dev": "browser-sync start --server --port 8080 --no-open --no-ui",
    "demo": "browser-sync start --server --port 8080 --no-open",
    "postinstall": "bower install"
